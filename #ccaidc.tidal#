cps (132 / 60 / 2)


d1 $ every 2 (slow 2) $ every 8 (|*| speed 1.5) $ juxBy 0.25 (rev . chop 2) $ distrib ["<3 5 3>","<7 5>",8] $ sound "osc1:1"
  # gain 1.1
  # legato 1
  # bpf (slow 4 $ scale 1000 3000 sine) # bpq 10

d2 $ every 4 (within (0.25,0.75) (hurry 2)) $ juxBy 0.25 rev $ off 0.25 (|*| speed 1.5) $ (chop 8 $ sound "osf1") # hpf (scale 100 8000 saw)
  # gain 1.1

d2 $ sound "cp" # gain 1.2

nudger (-0.01)

hush