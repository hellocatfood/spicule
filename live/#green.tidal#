d2 $ every 4 (0.25 <~) $ jux rev $ chunk 4 (hurry 2) $ slow 2 $ n "0(6,16)" # sound "kicklinn"
  # shape 0.8

d3 $ off 0.25 (# grunge 0.3) $ sound "drum*2 cp"
  # speed 2
  # orbit 1

cps 0.8

d1 $ slow 2 $ n "0 1" # sound "numbers" # pan "0 1"

d1 $ sound "bk:1(3,8)"


d1 $ slow 2 $ n (slow 2 $ "c(3,8) e(3,8) f(3,8) g*2") # s "supermandolin"
  # gain 1.1
  # sustain 1

