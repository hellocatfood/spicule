cps (126/60/2)

d1 $ every 4 (0.25 <~) $ whenmod 16 12 (|*| speed "[1,4]")
  $ up "0 ~ ~ 1" # sound "sid:1"
  # orbit 1
  # room (slow 4 $ scale 0.6 0.8 saw)
  # size 0.3
  # shape 0.8
  # gain 0.9

d2 $ whenmod 32 20 (off 0.125 (|*| speed 2))
  $ sound "kicklinn [~ kicklinn:1] ~ kicklinn"
  # orbit 2
  # room 0.4
  # size 0.7
  # grunge 0.4
  
d3 $ n "~ 40" # sound "pulse"
  # crush 2
  # gain 0.9

d4 $ n "5 2 [~ 9] 4" # sound "jvrave"