cps 0.8

m $ sometimes (|+| n 12) $ slow 2 $ off 0.25 (# velocity 0.23) $ note (off 0.125 (+7) $ "c4 ~ [a3 a4] g4")
  # cutoff sine
  # pitch1 saw
  # pitch2 0.5
  # attack rand
