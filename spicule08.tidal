d1 $ jux rev $ do let numbers = [2,3,4,5]
                      possibilities = permutations numbers
                      total = sum numbers
                      l = length possibilities
                  x <- discretise (1%(fromIntegral total)) $ irand l
                  spaceOut (possibilities !! x) $ sound "[bd sn mt*2 lt drum*3 sn:2*2 sn:4]"
