cps 0.75

d1 $ struct "[x _ x*2, ~ <x [~ x]> ~ x]" $ sound "bks:3"
  # legato 1

d2 $ (off "<0.125 0.25 0.75>" (# (shape 0.99 # gain 0.75)) $ sound "bk:1 ~ [~ bk:6] [~ bk:3]")
  # speed "[1]"
  # legato 0.5

d3 $ sound "[~ ~ cp8:3 ~] sd:1([1*8 3*8]/16,8)"
  # delay 0.9 # delaytime 0.01 # delayfb 0.6
  # legato 1

d4 $ jux rev $ chunk 4 (hurry 2) $ stack [struct "<x(5,8) x*4>" $ sound "kicklinn"
                                          # shape 0.9,
                                          loopAt 2 $ chop 16 $ sound "arc"
                                          # gain 1.3,
                                          n "b7 [~ d7] ~ e7" # sound "xx"
                                          # legato 1 # crush 5
                                          |+| n 0
                                         ]


d1 $ slow 4 $ jux rev $ n (off 0.125 (+12) $ off 0.25 (+7) $ slow 2 $ "c8(3,8) a6(3,8) f7(3,8) e7*2") # sound "xx"



d1 $ sound "gabba"

