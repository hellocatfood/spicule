

d1 $ drumstation $ off 0.25 (# dist 0) $ drum "[bd(<3 4 5>,8,<0 2>), sn*2]"
  # begin 0 # dist (1) # gain 1.2 # tune saw # snappy rand

d1 $ drumstation $ jux rev $ slicepat "0 1*2 <4 7 3> [5 6]" 8 $ drum "<bd cgl cgh sd>(<3 5>,8,<2 0 4>)"
  # gain 1.1
  # tune (slow 2 $ scale 0.25 1 sine)

d2 $ loopAt 2 $ slice (someCycles (+4) $ chunk 4 (fast 0.5) $ "0 [1 2] ~ 3*2 4 5*2 [ 6] 7") 8 $ sound "arc"
  # room 0 # legato 1 # gain 1.2

